// https://elixir.bootlin.com/linux/latest/source/arch/arm/include/uapi/asm/stat.h
typedef stat_t : {
    st_dev: isize,
    st_ino: isize,
    st_mode: u16,
    st_nlink: u16,
    st_uid: u16,
    st_gid: u16,
    st_rdev: isize,
    st_size: isize,
    st_blksize: isize,
    st_blocks: isize,
    st_atime: isize,
    st_atime_nsec: isize,
    st_mtime: isize,
    st_mtime_nsec: isize,
    st_ctime: isize,
    st_ctime_nsec: isize,
    __unused4: isize,
    __unused5: isize,
}
